<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>model.messages</base-name>
            <var>msg</var>
        </resource-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>userHandler</managed-bean-name>
        <managed-bean-class>controle.UserHandler</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>TccControle</managed-bean-name>
        <managed-bean-class>controle.TccControle</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>AlunoControle</managed-bean-name>
        <managed-bean-class>controle.AlunoControle</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ProfessorControle</managed-bean-name>
        <managed-bean-class>controle.ProfessorControle</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <!--Tcc-->
    
    <navigation-rule>
        <from-view-id>/paginas/tcc/novo.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/paginas/tcc/lista.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/paginas/tcc/lista.xhtml</to-view-id>
        </navigation-case>        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/tcc/lista.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/paginas/tcc/lista.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/tcc/editar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/paginas/tcc/lista.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/paginas/tcc/lista.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>tcc_novo</from-outcome>
            <to-view-id>/paginas/tcc/novo.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>tcc_list</from-outcome>
            <to-view-id>/paginas/tcc/lista.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/tcc/lista.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit_tcc</from-outcome>
            <to-view-id>/paginas/tcc/editar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <!--Acompanhamento-->
    
